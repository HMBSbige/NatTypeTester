{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"assignees": [
		"HMBSbige"
	],
	"dependencyDashboard": false,
	"extends": [
		"config:recommended",
		":configMigration",
		":automergeBranch",
		":automergeDigest",
		":automergeMinor",
		":disableRateLimiting"
	],
	"packageRules": [
		{
			"groupName": "ABP",
			"matchPackageNames": [
				"Volo.Abp{/,}**"
			]
		},
		{
			"groupName": "ReactiveUI",
			"matchPackageNames": [
				"ReactiveUI{/,}**"
			]
		}
	],
	"labels": [
		"Automatic"
	]
}
